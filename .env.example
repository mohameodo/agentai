# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key

# CSRF Protection (required)
CSRF_SECRET=your_32_character_random_string

# AI Model API Keys
OPENAI_API_KEY=sk-your_openai_api_key
MISTRAL_API_KEY=your_mistral_api_key
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
XAI_API_KEY=your_xai_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# Cloudflare Workers AI Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# TMDB Configuration (for movie/TV show search)
TMDB_API_KEY=your_tmdb_api_key

# Ollama Configuration (for local AI models)
OLLAMA_BASE_URL=http://localhost:11434

# Developer Tools (optional)
EXA_API_KEY=your_exa_api_key
GITHUB_TOKEN=your_github_token

# Production Configuration (optional)
NEXT_PUBLIC_VERCEL_URL=your_production_domain
NEXT_PUBLIC_APP_URL=https://ai.nexiloop.com

# Development Tools (optional)
ANALYZE=false

# Workspace directory for CodeHat (optional)
WORKSPACE_ROOT=/path/to/workspace

# OAuth Configuration (for GitHub and Google authentication)
# Get these from your Supabase project settings > Authentication > Providers
NEXT_PUBLIC_SUPABASE_GITHUB_CLIENT_ID=your_github_oauth_client_id
SUPABASE_GITHUB_CLIENT_SECRET=your_github_oauth_client_secret
NEXT_PUBLIC_SUPABASE_GOOGLE_CLIENT_ID=your_google_oauth_client_id
SUPABASE_GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret
